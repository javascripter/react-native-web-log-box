{
  "name": "react-native-web-log-box",
  "version": "0.0.7",
  "description": "A web replacement for React Native's LogBox",
  "main": "build/index.js",
  "types": "build/index.d.ts",
  "sideEffects": false,
  "scripts": {
    "build": "EXPO_NONINTERACTIVE=1 expo-module build",
    "build:watch": "expo-module build",
    "clean": "expo-module clean",
    "test": "expo-module test",
    "test:watch": "expo-module test",
    "prepare": "expo-module prepare",
    "prepublishOnly": "expo-module prepublishOnly",
    "expo-module": "expo-module",
    "dev:web": "expo web",
    "build:web": "npm run build && WEB_PUBLIC_URL=/ expo build:web",
    "clean:web": "npm run clean && rimraf ./web-build"
  },
  "author": "Tsubasa Sakai <javascripter@tsukkun.net> (https://github.com/javascripter)",
  "license": "MIT",
  "homepage": "https://github.com/javascripter/react-native-web-log-box#readme",
  "keywords": [
    "logbox",
    "react",
    "react-native",
    "react-native-web",
    "expo",
    "web"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/javascripter/react-native-web-log-box.git"
  },
  "bugs": {
    "url": "https://github.com/javascripter/react-native-web-log-box/issues"
  },
  "files": [
    "build"
  ],
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "@expo/webpack-config": "^0.12.74",
    "@types/react": "^17.0.8",
    "@types/react-native": "^0.64.7",
    "@typescript-eslint/eslint-plugin": "^4.25.0",
    "@typescript-eslint/parser": "^4.25.0",
    "eslint": "^7.27.0",
    "expo": "^41.0.1",
    "expo-cli": "^4.5.0",
    "expo-module-scripts": "^2.0.0",
    "prettier": "^2.3.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-native": "^0.64.1",
    "react-native-web": "^0.16.3",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.2"
  },
  "dependencies": {
    "invariant": "^2.2.4"
  }
}
